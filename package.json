{
  "name": "trello-clone",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "server:test": "cd server && npm run lint && npm run mocha",
    "server:lint": "cd server && eslint src/. test/. --config .eslintrc.json",
    "server:dev": "cd server && nodemon src/",
    "server:start": "cd server && node src/",
    "server:mocha": "cd server && mocha test/ --recursive --exit",

    "client:serve": "cd client && vue-cli-service serve",
    "client:build": "cd client && vue-cli-service build",
    "client:lint": "cd client && vue-cli-service lint"
  },
